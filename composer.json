{
    "name": "zaporylie/cargonizer",
    "description": "SDK for Cargonizer API by Logistra",
    "type": "library",
    "keywords": ["sdk", "api", "logistic"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Piasecki",
            "email": "jakub@piaseccy.pl"
        }
    ],
    "require": {
        "php": "^8.3",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "guzzlehttp/psr7": "^2.0"
    },
    "suggest": {
        "php-http/discovery": "For automatic discovery of HTTP clients and factories"
    },
    "require-dev": {
        "php-http/guzzle7-adapter": "^1.0",
        "php-coveralls/php-coveralls": "^2.4",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-type-coverage": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "rector/rector": "^1.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6"
    },
    "scripts": {
        "test": "pest --parallel",
        "lint": "parallel-lint . --exclude vendor",
        "rector": "rector process --dry-run",
        "rector:fix": "rector process",
        "pint": "pint --test",
        "pint:fix": "pint",
        "qa": [
            "@pint",
            "@rector",
            "@test"
        ],
        "post-update-cmd": "zaporylie\\Cargonizer\\ScriptHandler::createExamplesConfigFile",
        "post-install-cmd": "zaporylie\\Cargonizer\\ScriptHandler::createExamplesConfigFile"
    },
    "autoload": {
        "psr-4": {
            "zaporylie\\Cargonizer\\": "src/zaporylie/Cargonizer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
